---
interface Props {
  elements?: string[];
}

export const importantElements = [
  "anchor-gradient",
  "anchor-link",
  "button-glow",
  "button-icon",
  "code-highlighter",
  "container-layout",
  "example-section",
  "footer-section",
  "realm-demo",
  "realm-logo",
  "sb-viewer",
  "theme-provider",
  "top-navigation",
];

const { elements = [] } = Astro.props;
---

{
  elements.map((elementName) => (
    <import-element
      from={`${Astro.url.origin}/elements/${elementName}.element.html`}
    />
  ))
}
